{
    "pages": [
        {
            "path": "/account/:address",
            "name": "account-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Account Detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "address",
                            "accountAliasName",
                            "addressHeight",
                            "publicKey",
                            "importance",
                            "type",
                            "linkedAccountKey"
                        ],
                        "managerGetter": "account/info",
                        "pagination": false,
                        "errorMessage": "Account is unknown to the network."
                    },
                    {
                        "type": "Card",
                        "title": "Multisig Cosignatories",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "fields": [
                            "minApproval",
                            "minRemoval",
                            "cosignatories",
                            "multisigAccounts"
                        ],
                        "managerGetter": "account/multisig",
                        "pagination": false,
                        "errorMessage": "Address is not a multisig account."
                    },
                    {
                        "type": "Card",
                        "title": "Owned Mosaics",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "account/OwnedMosaic",
                        "fields": [
                            "mosaicId",
                            "amount",
                            "mosaicAliasName"
                        ],
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Owned Namespaces",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "account/OwnedNamespace",
                        "fields": [
                            "namespaceName",
                            "status",
                            "duration",
                            "registrationType"
                        ],
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Transactions",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "filter-tmp-disabled",
                                "filterOptionsGetter": "account/transactionFilterOptions",
                                "filterValueGetter": "account/transactionFilterValue",
                                "filterChangeAction": "account/setTransactionFilterValue"
                            }
                        ],
                        "body": "TableListView",
                        "fields": [
                            "deadline",
                            "transactionHash",
                            "transactionType",
                            "direction"
                        ],
                        "managerGetter": "account/transactions",
                        "timelinePagination": true
                    },
                    {
                        "type": "Card",
                        "title": "Account Partial",
                        "fullWidth": true,
                        "body": "TableListView",
                        "managerGetter": "account/partialTransactions",
                        "fields": [
                            "signer",
                            "transactionType",
                            "transactionHash",
                            "deadline",
                            "transactionBody"
                        ],
                        "timelinePagination": true
                    },
                    {
                        "type": "Card",
                        "title": "Account Restriction",
                        "fullWidth": true,
                        "body": "TableListView",
                        "managerGetter": "account/restrictions",
                        "pagination": false,
                        "pageSize": 5,
                        "errorMessage": "Account restrictions are not available."
                    },
                    {
                        "type": "Card",
                        "title": "Metadata Entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "account/metadatas",
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Importance History",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "recalculationBlock",
                            "totalFeesPaid",
                            "beneficiaryCount",
                            "importanceScore"
                        ],
                        "managerGetter": "account/info",
                        "dataGetter": "account/getActivityBucketList",
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        },
        {
            "path": "/block/:height",
            "name": "block-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "block_detail",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "Pagination",
                                "canFetchPrevious": true,
                                "canFetchNext": true,
                                "nextPageAction": "block/nextBlock",
                                "previousPageAction": "block/previousBlock"
                            }
                        ],
                        "body": "TableInfoView",
                        "fields": [
                            "height",
                            "date",
                            "total_fee",
                            "difficulty",
                            "fee_multiplier",
                            "transactions",
                            "harvester",
                            "block_hash"
                        ],
                        "pagination": false,
                        "managerGetter": "block/info",
                        "errorMessage": "block_info_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "block_transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "deadline",
                            "transaction_id",
                            "transaction_hash",
                            "type"
                        ],
                        "managerGetter": "block/blockTransactions",
                        "timelinePagination": true,
                        "errorMessage": "block_transactions_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "balance_change_receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "version",
                            "type",
                            "size",
                            "target_public_address",
                            "mosaic_id",
                            "amount"
                        ],
                        "dataGetter": "block/balanceChangeReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "block_balance_change_receipt_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "balance_transfer_receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "version",
                            "type",
                            "size",
                            "sender",
                            "recipient_address",
                            "mosaic_id",
                            "amount"
                        ],
                        "dataGetter": "block/balanceTransferReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "block_balance_transfer_receipt_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "inflation_receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "version",
                            "type",
                            "size",
                            "mosaic_id",
                            "amount"
                        ],
                        "dataGetter": "block/inflationReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "block_inflation_receipt_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "artifact_expiry_receipt",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "version",
                            "type",
                            "size",
                            "artifact_id"
                        ],
                        "dataGetter": "block/artifactExpiryReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "block_artifact_expiry_receipt_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "resolution_statement",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/resolutionStatement",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "block_resolution_statement_error_message"
                    }
                ]
            }
        },
        {
            "path": "/mosaic/:mosaicId",
            "name": "mosaic-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "mosaic_detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "mosaic_id",
                            "mosaic_alias_name",
                            "divisibility",
                            "address",
                            "supply",
                            "relative_amount",
                            "revision",
                            "start_height",
                            "duration",
                            "supply_mutable",
                            "transferable",
                            "restrictable"
                        ],
                        "managerGetter": "mosaic/info",
                        "pagination": false,
                        "errorMessage": "mosaic_info_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "mosaic_restriction",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "fields": [
                            "composite_hash",
                            "entry_type"
                        ],
                        "managerGetter": "mosaic/restrictions",
                        "pagination": false,
                        "errorMessage": "mosaic_restriction_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "mosaic_restriction_list",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "restriction_key",
                            "restriction_type",
                            "restriction_value"
                        ],
                        "managerGetter": "mosaic/restrictions",
                        "dataGetter": "mosaic/getMosaicRestrictionList",
                        "pagination": false,
                        "errorMessage": "mosaic_global_restrictions_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "metadata_entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadata_id",
                            "scoped_metadata_key",
                            "target_id",
                            "sender_address",
                            "target_address",
                            "metadata_value"
                        ],
                        "managerGetter": "mosaic/metadatas",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "mosaic_metadatas_error_message"
                    }
                ]
            }
        },
        {
            "path": "/namespace/:namespaceId",
            "name": "namespace-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "namespace_detail",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "owner_address",
                            "namespace_name",
                            "namespace_id",
                            "registration_type",
                            "status",
                            "alias_type",
                            "alias_address",
                            "alias_mosaic",
                            "start_height",
                            "expired_in_block",
                            "before_end_height",
                            "end_height"
                        ],
                        "managerGetter": "namespace/info",
                        "pagination": false,
                        "errorMessage": "namespace_info_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "namespace_level",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "name",
                            "namespace_id",
                            "parent_id"
                        ],
                        "managerGetter": "namespace/namespaceLevel",
                        "pagination": false,
                        "errorMessage": "namespace_level_error_message"
                    },
                    {
                        "type": "Card",
                        "title": "metadata_entries",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadata_id",
                            "scoped_metadata_key",
                            "target_id",
                            "sender_address",
                            "target_address",
                            "metadata_value"
                        ],
                        "managerGetter": "namespace/metadatas",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "namespace_metadatas_error_message"
                    }
                ]
            }
        },
        {
            "path": "/transaction/:transactionHash",
            "name": "transaction-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "Transaction Info",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "blockHeight",
                            "transactionHash",
                            "transactionId",
                            "date",
                            "deadline",
                            "effectiveFee",
                            "signature",
                            "signer",
                            "status",
                            "confirm"
                        ],
                        "managerGetter": "transaction/info",
                        "pagination": false,
                        "errorMessage": "Transaction does not exist."
                    },
                    {
                        "type": "Card",
                        "title": "Transaction Detail",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/transactionDetail",
                        "pagination": false
                    },
                    {
                        "type": "Card",
                        "title": "Mosaics",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "mosaicId",
                            "amount",
                            "mosaicAliasName"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/transferMosaics",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Aggregate Inner Transactions",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "transactionId",
                            "type",
                            "signer",
                            "transactionBody"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/aggregateInnerTransactions",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    },
                    {
                        "type": "Card",
                        "title": "Aggregate Cosignatures",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "signature",
                            "signer"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/aggregateCosignatures",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5
                    }
                ]
            }
        }
    ]
}