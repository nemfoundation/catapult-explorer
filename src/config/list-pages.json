{
    "pages": [
        {
            "path": "/blocks",
            "name": "blocks",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "blocks",
                "hasInfoText": true,
                "storeNamespaces": ["block"],
                "managerGetter": "block/timeline",
                "infoTextGetter": "block/infoText",
                "Fields": [
                    "height",
                    "age",
                    "transactions",
                    "total_fee",
                    "date",
                    "harvester"
                ],
                "mobileFields": [
                    "height",
                    "age",
                    "transactions",
                    "total_fee"
                ]
            }
        },
        {
            "path": "/transactions",
            "name": "transactions",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "transactions",
                "storeNamespaces": ["transaction"],
                "managerGetter": "transaction/timeline",
                "hasFilter": true,
                "Fields": [
                    "height",
                    "deadline",
                    "transaction_hash",
                    "transaction_descriptor",
                    "max_fee",
                    "signer",
                    "recipient"
                ],
                "mobileFields": [
                    "height",
                    "transaction_hash",
                    "fee",
                    "signer",
                    "recipient"
                ]
            }
        },
        {
            "path": "/accounts",
            "name": "accounts",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "accounts",
                "hasFilter": true,
                "managerGetter": "account/timeline",
                "storeNamespaces": ["account"],
                "Fields": [
                    "address",
                    "balance",
                    "last_activity",
                    "importance",
                    "account_alias_name"
                ],
                "mobileFields": [
                    "address",
                    "importance",
                    "info"
                ]
            }
        },
        {
            "path": "/namespaces",
            "name": "namespaces",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "namespaces",
                "storeNamespaces": ["namespace"],
                "managerGetter": "namespace/timeline",
                "Fields": [
                    "namespace_name",
                    "namespace_id",
                    "owner_address",
                    "duration",
                    "approximate_expired"
                ],
                "mobileFields": [
                    "namespace_name",
                    "owner_address",
                    "duration"
                ]
            }
        },
        {
            "path": "/mosaics",
            "name": "mosaics",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "mosaics",
                "storeNamespaces": ["mosaic"],
                "managerGetter": "mosaic/timeline",
                "Fields": [
                    "mosaic_id",
                    "mosaic_alias_name",
                    "owner_address",
                    "supply",
                    "divisibility",
                    "relative_amount",
                    "start_height"
                ],
                "mobileFields": [
                    "mosaic_id",
                    "mosaic_alias_name",
                    "owner_address"
                ]
            }
        },
        {
            "path": "/nodes",
            "name": "nodes",
            "meta": {
                "group": "list-page",
                "keepAliveGoTo": ["detail-page"]
            },
            "props": {
                "title": "nodes",
                "storeNamespaces": ["node"],
                "managerGetter": "node/timeline",
                "Fields": [
                    "index",
                    "version",
                    "roles",
                    "network",
                    "host",
                    "port",
                    "address",
                    "friendly_name"
                ],
                "mobileFields": [
                    "index",
                    "roles",
                    "address",
                    "friendly_name",
                    "host"
                ]
            }
        }
    ]
}
